programa {
  funcao inicio() {
    
    cadeia nomeUm
    inteiro numeroUM
    cadeia nomeDois

    escreva("Olá qual o seu nome? ")
    leia(nomeUm)

    escreva("E qual a sua idade? ")
    leia(numeroUM)

    escreva("E qual seria seu endereço? ")
    leia(nomeDois)

    escreva("\nNome digitado: ", nomeUm)
    escreva("\nIdade digitada: ", numeroUM)
    escreva("\nEndereço digitado: ", nomeDois)
  }
}
